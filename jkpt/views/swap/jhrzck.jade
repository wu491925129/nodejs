                                        p.sm-text 2017-3-20
                            .col-sm-3
                                .col-xs-5.btn-success
                                    p.p-text 法人资质信息
                                    .circle
                                        img.icon-size(src="/assets/img/1.png", alt="")
                                .col-xs-8.btn-light1
                                    .col-md-12.text
                                        span 864532
                                        span.sm-text 条
                                    .col-md-12
                                        span
                                            strong 数据最后更新时间:
                                    .col-md-12
                                        p.sm-text 2017-3-20
                            .col-sm-3
                                .col-xs-5.btn-purple
                                    p.p-text 法人登记信息
                                    .circle
                                        img.icon-size(src="/assets/img/8.png", alt="")
                                .col-xs-8.btn-light1
                                    .col-md-12.text
                                        span 784658
                                        span.sm-text 条
                                    .col-md-12
                                        span
                                            strong 数据最后更新时间:
                                    .col-md-12
                                        p.sm-text 2017-3-20
                            .col-sm-3
                                .col-xs-5.btn-warning
                                    p.p-text 法人处罚信息
                                    .circle
                                        img.icon-size(src="/assets/img/2.png", alt="")
                                .col-xs-8.btn-light1
                                    .col-md-12.text
                                        span 8848
                                        span.sm-text 条
                                    .col-md-12
                                        span
                                            strong 数据最后更新时间:
                                    .col-md-12
                                        p.sm-text 2017-3-20
                    .row.panel
                        .col-md-12
                            .panel(style="background-color:#f5f5f5")
                                h4.h4text 房屋库日志
                        .col-md-12
                            .col-sm-3
                                .col-xs-5.btn-danger
                                    p.p-text 房屋基本信息
                                    .circle
                                        img.icon-size(src="/assets/img/10.png", alt="")
                                .col-xs-8.btn-light1
                                    .col-md-12.text
                                        span 583453
                                        span.sm-text 条
                                    .col-md-12
                                        span
                                            strong 数据最后更新时间:
                                    .col-md-12
                                        p.sm-text 2017-3-20
                            .col-sm-3
                                .col-xs-5.btn-warning
                                    p.p-text 人户分离信息
                                    .circle
                                        img.icon-size(src="/assets/img/12.png", alt="")
